{
  "name": "typed-wamp",
  "version": "0.2.1",
  "description": "A set of type definitions and convenience classes for working with WAMP messages.",
  "main": "index.js",
  "repository": "https://github.com/GregRos/typed-wamp",
  "author": "GregRos",
  "license": "MIT",
  "typings": "index",
  "dependencies": {},
  "keywords": [
    "wamp",
    "typescript"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist/",
    "build:clean": "npm-run-all clean build",
    "test": "nyc ava",
    "lint": "tslint -p .",
    "lint:fix": "tslint -p . --fix",
    "package:publish": "npm-run-all lint build:clean test && node dist/publish.js && (cd .publish; yarn publish)",
    "docs": "node dist/generate-docs/run.js"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.148",
    "@types/shelljs": "^0.8.6",
    "ava": "^2.4.0",
    "codecov": "^3.6.1",
    "globby": "^10.0.1",
    "nyc": "^15.0.0",
    "retarget-sourcemaps-after-move": "^0.1.1",
    "shelljs": "^0.8.3",
    "source-map-support": "^0.5.16",
    "tslint": "^5.20.1",
    "tslint-config-gregros": "^1.3.0",
    "typedoc": "^0.15.6",
    "typedoc-plugin-example-tag": "^1.0.2",
    "typedoc-plugin-external-module-name": "^2.1.0",
    "typedoc-plugin-internal-external": "^2.0.2",
    "typescript": "^3.7.2"
  },
  "ava": {
    "files": [
      "dist/test/**/*.spec.js"
    ],
    "verbose": true
  },
  "browserslist": [
    "last 1 Chrome versions"
  ],
  "nyc": {
    "include": [
      "dist/lib/**/*.js",
      "src/lib/**/*.ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": true,
    "extension": [
      ".ts"
    ]
  },
  "sideEffects": false
}
